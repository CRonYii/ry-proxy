{
	"log": {
		"loglevel": "warning",
		"access": "/dev/stdout",
		"error": "/var/log/v2ray/error.log"
	},
	"inbounds": [
		{
			"port": $inbound_port,
			"protocol": "vmess",
			"settings": {
				"clients": [
					{
						"id": "$uuid",
						"alterId": $alter_id
					}
				]
			},
			"streamSettings": {
				"network": "h2",
				"security": "tls",
				"httpSettings": {
					"path": "/livadata",
					"host": [
						"localhost"
					]
				}
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"settings": {}
		}
	]
}
