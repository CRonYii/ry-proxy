{
	"log": {
		"loglevel": "warning",
		"access": "/dev/null",
		"error": "/var/log/v2ray/error.log"
	},
	"inbounds": [
		{
			"port": $inbound_port,
			"protocol": "vmess",
			"settings": {
				"clients": [
					{
						"id": "$uuid",
						"alterId": 64
					}
				]
			},
			"streamSettings": {
				"network": "tcp",
				"tcpSettings": {
					"hedaer": {
						"type": "http",
						"response": {
							"version": "1.1",
							"status": "200",
							"reason": "OK",
							"headers": {
								"Content-Type": ["application/octet-stream", "application/x-msdownload", "text/html", "application/x-shockwave-flash"],
								"Transfer-Encoding": ["chunked"],
								"Connection": ["keep-alive"],
								"Pragma": "no-cache"
							}
						}
					}
				}
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"settings": {}
		}
	]
}
