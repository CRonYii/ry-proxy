{
	"log": {
		"loglevel": "warning",
		"access": "/dev/null",
		"error": "/var/log/v2ray/error.log"
	},
	"inbounds": [
		{
			"port": $inbound_port,
			"protocol": "vmess",
			"settings": {
				"clients": [
					{
						"id": "$uuid",
						"alterId": 32
					}
				]
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"settings": {}
		}
	]
}
